{
  "name": "overwatch-team-composition-generator",
  "version": "1.0.0",
  "description": "Team composition generator for Overwatch",
  "author": "Patrick Szela (https://github.com/PatrickSzela)",
  "private": true,
  "scripts": {
    "dev": "modernizr -c modernizr-config.json -d plugins; nuxt",
    "build": "modernizr -c modernizr-config.json -d plugins; nuxt build",
    "build:gh-pages": "DEPLOY_ENV=GH_PAGES nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "generate:gh-pages": "DEPLOY_ENV=GH_PAGES nuxt generate",
    "lint": "eslint --ext .ts,.js,.vue --ignore-path .gitignore .",
    "deploy": "push-dir --dir=dist --branch=gh-pages --cleanup"
  },
  "dependencies": {
    "@aceforth/nuxt-optimized-images": "^1.0.0",
    "@types/webpack": "^4.41.12",
    "@types/webpack-env": "^1.15.2",
    "material-components-vue": "^1.2.0",
    "normalize.css": "^8.0.1",
    "nuxt": "^2.12.2"
  },
  "devDependencies": {
    "@nuxt/typescript-build": "^0.6.6",
    "@nuxtjs/eslint-config-typescript": "^1.0.2",
    "@nuxtjs/eslint-module": "^1.2.0",
    "@types/modernizr": "^3.5.3",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-nuxt": "^0.5.2",
    "eslint-plugin-prettier": "^3.1.3",
    "fibers": "^5.0.0",
    "imagemin": "^7.0.1",
    "imagemin-mozjpeg": "^8.0.0",
    "imagemin-pngquant": "^8.0.0",
    "imagemin-svgo": "^7.1.0",
    "lqip-loader": "^2.2.0",
    "node-sass": "^4.14.0",
    "prettier": "^2.0.5",
    "push-dir": "^0.4.1",
    "responsive-loader": "^1.2.0",
    "sass": "^1.26.5",
    "sass-loader": "^8.0.2",
    "sharp": "^0.25.2",
    "webp-loader": "^0.6.0",
    "webpack-cli": "^3.3.11"
  }
}
